{
  "name": "@oscaner/cache-manager-adapter",
  "version": "17.0.0",
  "description": "Adapter for using cache-manager with type-cacheable",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "start": "node ./dist/index.js",
    "test": "jest --coverage --runInBand",
    "test:watch": "jest --watch --runInBand",
    "build": "npm run clean && tsc -p ./tsconfig.json",
    "prepare": "npm run-script build",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Oscaner/type-cacheable-cache-manager-adapter.git"
  },
  "keywords": [
    "cache",
    "typescript",
    "keyv"
  ],
  "author": {
    "name": "Oscaner Miao",
    "url": "https://github.com/Oscaner"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Oscaner/type-cacheable-cache-manager-adapter/issues"
  },
  "homepage": "https://github.com/Oscaner/type-cacheable-cache-manager-adapter#readme",
  "devDependencies": {
    "@resolid/keyv-sqlite": "github:Oscaner/keyv-sqlite#main",
    "@types/jest": "^29",
    "better-sqlite3": "^11",
    "cacheable": "^1",
    "jest": "^29",
    "lru-cache": "^11.0.2",
    "ts-jest": "^29",
    "typescript": "^5"
  },
  "dependencies": {
    "@type-cacheable/core": "^14",
    "cache-manager": "^6",
    "keyv": "^5",
    "wildcard-match": "^5"
  }
}
